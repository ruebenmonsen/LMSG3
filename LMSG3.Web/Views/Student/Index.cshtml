    <hr />
    <dl class="row">
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.CourseInfo.Name)
        </dt>
        <dd class="col-sm-10">
            @Model.CourseInfo.Name
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.CourseInfo.Description)
